      ]
     },
     "execution_count": 47,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "def get_info(row):\n",
    "    rank = row.find_all('tr')[1].find('td').text\n",
    "    title = row.find_all('tr')[1].find('a').text.strip()\n",
    "    link = row.find_all('tr')[1].find('a')['href']\n",
    "    num_players = row.find_all('tr')[1].find(class_='num').text\n",
    "    peak_concurrent_players = row.find_all('tr')[1].find(class_=\"num period-col peak-concurrent\").text\n",
    "    player_hours = row.find_all('tr')[1].find(class_=\"num period-col player-hours\").text\n",
    "    \n",
    "    output = {\n",
    "        'rank': rank,\n",
    "        'title':title,\n",
    "        'link':link,\n",
    "        'current_players':num_players,\n",
    "        'peak_players':peak_concurrent_players,\n",
    "        'player_hours':player_hours\n",
    "    }\n",
    "    \n",
    "    return output\n",
    "    \n",
    "get_info(tables[0])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 84,
   "id": "9795b79b",
   "metadata": {},
   "outputs": [],
   "source": [
    "info = []\n",
    "for table in tables:\n",
    "    for i in range(0, 151, 6):\n",
    "        info.append(table.find_all('td')[i:i+6])"
   ]
  },
