     "execution_count": 241,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "def get_steam_data(row):\n",
    "    try:\n",
    "        game_name = row.find(class_=\"apphub_AppName\").text\n",
    "        developer = row.find_all(class_='grid_content')[0].text.strip().split(',')\n",
    "        publisher = row.find_all(class_='grid_content')[1].text.strip()\n",
    "        release_date = row.find(class_=\"grid_content grid_date\").text.strip()\n",
    "        game_description = row.find(class_=\"game_description_snippet\").text.strip()\n",
    "        recent_reviews = row.find_all(class_='responsive_hidden')[0].text.strip().strip('()')\n",
    "        pct_positive = row.find_all(class_=\"nonresponsive_hidden responsive_reviewdesc\")[0].text.strip().split('%')[0].strip('- ')\n",
    "        all_reviews = row.find(class_=\"responsive_reviewdesc_short\").text.strip().split(')')[0] \n",
    "#         row.find_all(class_='responsive_hidden')[1].text.strip().strip('()')\n",
    "#         pct_all_positive = row.find_all(class_=\"nonresponsive_hidden responsive_reviewdesc\")[1].text.strip().split('%')[0].strip('- ')\n",
    "\n",
    "        output = {\n",
    "            'game_name': game_name,\n",
    "            'developer': developer,\n",
    "            'publisher': publisher,\n",
    "            'release_date': release_date,\n",
    "            'game_description': game_description,\n",
    "            'recent_reviews': recent_reviews,\n",
    "            'pct_positive': pct_positive,\n",
    "            'all_reviews': all_reviews,\n",
    "#             'pct_all_positive': pct_all_positive\n",
    "        }\n",
    "\n",
    "        return output\n",
    "    \n",
    "    except Exception as e:\n",
    "        print('-' * 30)\n",
    "        print(game_name, e, sep='\\n\\n')\n",
    "        print('*' * 30)\n",
    "\n",
    "get_steam_data(steam_info[0])"
   ]
  },
