    {
     "data": {
      "text/plain": [
       "{'rank': '1.',\n",
       " 'title': 'Counter-Strike: Global Offensive',\n",
       " 'link': '/app/730',\n",
       " 'current_players': '715968',\n",
       " 'peak_players': '906670',\n",
