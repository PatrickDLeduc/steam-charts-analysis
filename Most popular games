   "source": [
    "urls = df['link'].apply(lambda x: 'https://store.steampowered.com' + x)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 146,
   "id": "476997fc",
   "metadata": {},
   "outputs": [],
   "source": [
    "steam_info = []\n",
    "\n",
    "for url in urls:\n",
    "    r = requests.get(url)\n",
    "    soup = BeautifulSoup(r.content)\n",
    "    steam_page = soup.find(class_='glance_ctn')\n",
    "    steam_info.append(steam_page)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 186,
   "id": "9890d925",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "'88'"
      ]
     },
     "execution_count": 186,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "steam_info[0].find_all(class_=\"nonresponsive_hidden responsive_reviewdesc\")[1].text.strip().split('%')[0].strip('- ')"
   ]
  },
