   "cell_type": "code",
   "execution_count": 126,
   "id": "3cc3c2fb",
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_info(row):\n",
    "    rank = row[0].text\n",
    "    title = row[1].text.strip()\n",
    "    link = row[1].find('a')['href']\n",
    "    num_players = row[2].text\n",
    "    peak_concurrent_players = row[4].text\n",
    "    player_hours = row[5].text\n",
    "    \n",
    "    output = {\n",
    "        'rank': rank,\n",
    "        'title':title,\n",
    "        'link':link,\n",
    "        'current_players':num_players,\n",
    "        'peak_players':peak_concurrent_players,\n",
    "        'player_hours':player_hours\n",
    "    }\n",
    "    \n",
    "    return output"
   ]
